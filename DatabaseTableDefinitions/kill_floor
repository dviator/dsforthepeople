--The purpose of this table is to contain all of the data from the raw data set pulled from the following: http://aiddata.org/


CREATE TABLE KILL_FLOOR.AID_DATA_RAW 
(	
aiddata_id VARCHAR(20) Not NULL ,
year VARCHAR(10) Not NULL ,
commitment_date VARCHAR(20) NULL ,
start_date VARCHAR(20) NULL ,
end_date VARCHAR(20) NULL ,
donor VARCHAR(100) Not NULL ,
donor_iso2 VARCHAR(5) NULL ,
donor_iso3 VARCHAR(5) NULL ,
umbrella VARCHAR(20) NULL ,
donor_project_id VARCHAR(100) NULL ,
crsid VARCHAR(20) NULL ,
financing_agency VARCHAR(50) NULL ,
donor_type VARCHAR(20) NULL ,
bi_multi VARCHAR(3) NULL ,
recipient VARCHAR(100) NULL ,
recipient_iso2 VARCHAR(2) NULL ,
recipient_iso3 VARCHAR(3) NULL ,
private_recipient VARCHAR(100) NULL ,
implementing_agency VARCHAR(500) NULL ,
channel_code VARCHAR(20) NULL ,
borrower VARCHAR(100) NULL ,
beneficiary VARCHAR(100) NULL ,
guarantor VARCHAR(100) NULL ,
other_involved_institutions VARCHAR(20) NULL ,
commitment_amount VARCHAR(30) NULL ,
commitment_amount_currency VARCHAR(5) NULL ,
commitment_amount_usd_nominal VARCHAR(30) NULL ,
commitment_amount_usd_constant VARCHAR(30) NULL ,
disbursement_amount VARCHAR(30) NULL ,
disbursement_amount_currency VARCHAR(5) NULL ,
disbursement_amount_usd_nominal VARCHAR(30) NULL ,
disbursement_amount_usd_constant VARCHAR(30) NULL ,
total_project_cost VARCHAR(30) NULL ,
total_project_cost_currency VARCHAR(5) NULL ,
total_project_cost_usd_nominal VARCHAR(30) NULL ,
total_project_cost_usd_constant VARCHAR(30) NULL ,
title VARCHAR(200) NULL ,
short_description VARCHAR(500) NULL ,
short_description_original_language VARCHAR(20) NULL ,
long_description VARCHAR(750) NULL ,
long_description_original_language VARCHAR(20) NULL ,
additional_info VARCHAR(500) NULL ,
additional_info_original_language VARCHAR(20) NULL ,
flow_type VARCHAR(50) NULL ,
crs_flow_name VARCHAR(50) NULL ,
number_repayments_per_year VARCHAR(5) NULL ,
repay_type VARCHAR(5) NULL ,
loan_term VARCHAR(5) NULL ,
grace_period VARCHAR(5) NULL ,
interest_rate VARCHAR(30) NULL ,
second_interest_rate VARCHAR(30) NULL ,
grant_element VARCHAR(5) NULL ,
cancelled VARCHAR(5) NULL ,
repay_date_first VARCHAR(20) NULL ,
repay_date_last VARCHAR(20) NULL ,
untied_amount_usd_nominal VARCHAR(30) NULL ,
partial_tied_amount_usd_nominal VARCHAR(30) NULL ,
tied_amount_usd_nominal VARCHAR(30) NULL ,
received_amount_usd_nominal VARCHAR(30) NULL ,
irtc_amount_usd_nominal VARCHAR(30) NULL ,
expert_commitment_amount_usd_nominal VARCHAR(30) NULL ,
expert_extended_amount_usd_nominal VARCHAR(30) NULL ,
export_credit_amount_usd_nominal VARCHAR(30) NULL ,
outstanding_amount_usd_nominal VARCHAR(30) NULL ,
arrears_principal_amount_usd_nominal VARCHAR(30) NULL ,
arrears_interest_amount_usd_nominal VARCHAR(30) NULL ,
future_ds_principal_amount_usd_nominal VARCHAR(30) NULL ,
future_ds_interest_amount_usd_nominal VARCHAR(30) NULL ,
region VARCHAR(100) NULL ,
location VARCHAR(100) NULL ,
source VARCHAR(50) Not NULL ,
language VARCHAR(20) Not NULL ,
biodiversity VARCHAR(5) NULL ,
climate VARCHAR(5) NULL ,
desertification VARCHAR(5) NULL ,
crs_purpose_code VARCHAR(10) Not NULL ,
crs_purpose_name VARCHAR(300) NULL ,
gender VARCHAR(5) NULL ,
trade VARCHAR(5) NULL ,
pdgg VARCHAR(5) NULL ,
ftc VARCHAR(5) NULL ,
sector_programme VARCHAR(20) NULL ,
sector VARCHAR(500) NULL ,
sector_code VARCHAR(10) NULL ,
associated_financing VARCHAR(5) NULL ,
initial_report VARCHAR(5) NULL ,
finance_t VARCHAR(5) NULL ,
environmental_impact_assessment VARCHAR(5) NULL ,
environment VARCHAR(5) NULL ,
investment_project VARCHAR(5) NULL ,
aiddata_purpose_code VARCHAR(10) NULL ,
aiddata_purpose_name VARCHAR(500) NULL ,
aiddata_activity_code VARCHAR(50) NULL ,
aiddata_activity_name VARCHAR(500) NULL ,
aiddata_feasibility_study VARCHAR(5) NULL ,
aiddata_technical_assistance VARCHAR(5) NULL ,
coalesced_purpose_code VARCHAR(50) NULL ,
coalesced_purpose_name VARCHAR(500) NULL ,
aiddata_is_conflict VARCHAR(5) NULL );
